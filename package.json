{
  "name": "@nuxtjs/style-resources",
  "version": "1.2.1",
  "description": "",
  "license": "MIT",
  "contributors": [
    {
      "name": "Alexander Lichter <npm@lichter.io>"
    }
  ],
  "main": "lib/module.js",
  "repository": {
    "mode": "git",
    "url": "git+https://github.com/nuxt-community/style-resources-module"
  },
  "bugs": {
    "url": "https://github.com/nuxt-community/style-resources-module/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt dev --config-file test/fixture/sass/nuxt.config.js",
    "lint": "eslint --ext .js,.ts,.vue lib test types",
    "test": "yarn lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish",
    "commitlint": "commitlint -e $GIT_PARAMS",
    "coverage": "codecov",
    "prepare": "husky install"
  },
  "types": "types/index.d.ts",
  "eslintIgnore": [
    ".nuxt",
    "coverage",
    "dist",
    "lib/templates/*.*",
    "node_modules"
  ],
  "files": [
    "lib",
    "types"
  ],
  "keywords": [
    "nuxtjs",
    "nuxt",
    "nuxt-module",
    "sass",
    "less",
    "stylus",
    "style-resources",
    "import"
  ],
  "engines": {
    "node": ">=v10.24.1",
    "npm": ">=6.14.15"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  },
  "dependencies": {
    "consola": "^2.15.3",
    "glob-all": "^3.2.1",
    "sass-resources-loader": "^2.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@types/jest": "^27.0.2",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "get-port": "^5.1.1",
    "husky": "^7.0.2",
    "jest": "^27.3.0",
    "jsdom": "^16.7.0",
    "less": "^4.1.2",
    "less-loader": "^4.1.0",
    "mathsass": "^0.11.0",
    "nuxt-edge": "^2.16.0-27226092.034b9901",
    "sass": "^1.43.2",
    "sass-loader": "^7.3.1",
    "standard-version": "^9.3.2",
    "stylus": "^0.55.0",
    "stylus-loader": "^3.0.2",
    "typescript": "^4.4.4"
  }
}
